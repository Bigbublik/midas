{
  "extends": [
    "config:base",
    "schedule:weekends",
    ":disableDependencyDashboard",
  ],
  "rangeStrategy": "replace",
  "lockFileMaintenance": {
    "enabled": true,
    "extends": ["schedule:weekends"]
  },
  "vulnerabilityAlerts": {
    "enabled": true,
    "extends": ["schedule:weekends"]
  },
  "assignees": ["hiroaki-yamamoto"],
  "reviewers": ["hiroaki-yamamoto"],
  "groupName": "Updates",
  "automerge": true,
  "major": {
    "automerge": false,
    "groupName": "Major Updates",
    "extends": ["schedule:weekends"]
  },
  "packageRules": [
    {
      "packageNames": ["time"],
      "allowedVersions": "<0.3"
    }
  ]
}
